[ignore]
.*/node_modules/.*
.*/demos/.*
.*/dist/.*
[include]
.*/src/.*
[libs]
flow
flow-typed
node_modules/chimee-helper/lib/index.flow.js
node_modules/chimee-kernel/lib/index.flow.js
node_modules/toxic-predicate-functions/lib/index.flow.js
[lints]
all=warn
untyped-type-import=error
sketchy-null-bool=off
[options]
module.name_mapper='^helper/\(.*\)$' -> '<PROJECT_ROOT>/src/helper/\1'
module.name_mapper='^dispatcher/\(.*\)$' -> '<PROJECT_ROOT>/src/dispatcher/\1'
module.name_mapper='^global/\(.*\)$' -> '<PROJECT_ROOT>/src/global/\1'
module.name_mapper='^config/\(.*\)$' -> '<PROJECT_ROOT>/src/config/\1'
esproposal.decorators=ignore
